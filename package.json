{
  "name": "react-blue",
  "version": "1.0.0",
  "description": "React prototyping tool for creating file structure",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open",
    "build": "NODE_ENV=production webpack --history-api-fallback",
    "dev": "NODE_ENV=development webpack-dev-server --open --history-api-fallback",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
